    "@semantic-release/github": "^10.3.4",
    "@semantic-release/npm": "^12.0.1",
    "@types/cookie": "^0.6.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash-es": "^4.17.12",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "semantic-release": "^24.1.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.5.0",
    "vite": "^5.4.1",
    "vitepress": "^1.3.4",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "cookie": "^0.6.0",
    "date-fns": "^3.6.0",
    "js-yaml": "^4.1.0",
    "lodash-es": "^4.17.21"
  }
