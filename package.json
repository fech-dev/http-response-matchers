{
  "name": "vitest-response-matchers",
  "description": "Custom matchers for validating HTTP responses in testing environments",
  "version": "1.0.2",
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "main": "dist/index.js",
  "types": "src/index.d.ts",
  "keywords": [
    "vitest",
    "jest",
    "matchers",
    "expect.extend",
    "expect",
    "response",
    "http"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
