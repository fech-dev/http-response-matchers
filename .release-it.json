{
    "$schema": "https://unpkg.com/release-it@17/schema/release-it.json",
    "git": {
      "commitMessage": "chore: release v${version}"
    },
   
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "header": "# Changelog",
        "preset": {
          "name": "conventionalcommits"
        }
      }
    }
  }